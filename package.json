{
  "name": "micro-node-launcher",
  "version": "0.2.1",
  "description": "A microservice launcher based on AMQP",
  "main": "build/index.js",
  "bin": {
    "micro": "bin/micro"
  },
  "scripts": {
    "pretest": "npm run build",
    "test": "DEBUG=amqp mocha",
    "lint": "eslint lib/*.js",
    "build": "babel -d ./build/ ./lib/",
    "prepublish": "npm run build",
    "micro": "./bin/micro ./test/services/fibonacci/index.js rpc_queue 127.0.0.1"
  },
  "config": {
    "ghooks": {
      "pre-push": "npm test",
      "pre-commit": "npm run lint",
      "post-merge": "npm install",
      "post-rewrite": "npm install"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/micro-node/launcher.git"
  },
  "keywords": [
    "micro-node",
    "launcher"
  ],
  "author": "Haithem Bel Haj <haythem.belhaj@gmail.com> (http://haithembelhaj.github.io)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/micro-node/launcher/issues"
  },
  "homepage": "https://github.com/micro-node/launcher#readme",
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-preset-es2015": "^6.5.0",
    "babel-register": "^6.5.1",
    "eslint": "^1.10.3",
    "ghooks": "^1.0.1",
    "mocha": "^2.3.4",
    "node-uuid": "^1.4.7"
  },
  "dependencies": {
    "colors": "^1.1.2",
    "lodash": "^3.10.1",
    "micro-node-amqp": "^1.1.4",
    "micro-node-json-rpc": "^1.1.0"
  }
}
